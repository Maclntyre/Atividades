 mkdir unix-tests 
cd unix-tests
touch trybe.txt
cp trybe.txt trybe-backup.txt
mv trybe.txt Trybe.txt
mkdir backup
mv trybe-backup.txt backup
mkdir backup2
cd ..
mv backup/trybe-backup.txt backup2
rm -rf backup
mv backup2 backup
ls : backup Trybe.txt
rm -rf backup
clear


